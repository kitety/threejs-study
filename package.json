{
  "private": true,
  "scripts": {
    "start": "cross-env UMI_ENV=dev umi dev  ",
    "build": "cross-env umi build ",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.32.3",
    "@three-ts/orbit-controls": "^1.4.7",
    "ahooks": "^3.1.9",
    "antd": "^4.18.5",
    "cross-env": "^7.0.3",
    "react": "^17.0.2",
    "react-dat-gui": "^4.1.1",
    "react-dom": "17.x",
    "stats.js": "^0.17.0",
    "three": "^0.136.0",
    "three-orbitcontrols-ts": "^0.1.2",
    "umi": "^3.5.20"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/stats.js": "^0.17.0",
    "@types/three": "^0.136.1",
    "@umijs/preset-react": "^2.1.1",
    "@umijs/test": "^3.5.20",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
